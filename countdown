
import java.util.*;

public class countdown
{
   public static void countdown() 
   {
      Stack<Integer> stack = new Stack<Integer>();
      for (int i = 1; i <= 3; i++)
      {
         stack.push(i);
      }
      while (!stack.empty()) 
      {
         System.out.println(stack.pop());
      }
         System.out.println("WELCOME TO BLACKJACK!");
         System.out.println(" " );
   }
}
