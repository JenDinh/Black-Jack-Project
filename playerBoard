import java.util.*;

public class playerboard
{
   static int position = 1;
   public static void playerboard(playerName player1)
   {
      Map<Integer, playerName> highscorelist = new HashMap<Integer, playerName>();
      highscorelist.put(position, player1); 
      System.out.println("");
      System.out.println("Player Attempts: " );
      System.out.println(highscorelist);
      System.out.println("");
      position++;
   }
}
